{
  "name": "@antdv-next/cssinjs",
  "type": "module",
  "version": "1.0.0-rc.1",
  "description": "cssinjs solution for ant-design-vue",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./cssinjs-utils": {
      "types": "./dist/cssinjs-utils/index.d.mts",
      "import": "./dist/cssinjs-utils/index.mjs",
      "default": "./dist/cssinjs-utils/index.mjs"
    },
    "./dist/*": {
      "types": "./dist/*.d.mts",
      "import": "./dist/*.mjs",
      "default": "./dist/*.mjs"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublish": "pnpm build",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "bump": "bumpp --commit \"chore(release): @antdv-next/cssinjs %s\" --push --tag \"@antdv-next/cssinjs@%s\""
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "@emotion/hash": "catalog:prod",
    "@emotion/unitless": "catalog:prod",
    "@v-c/util": "catalog:vc",
    "csstype": "catalog:prod",
    "defu": "catalog:prod",
    "stylis": "catalog:prod"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "catalog:dev",
    "@types/node": "catalog:types",
    "@types/stylis": "catalog:types",
    "@types/testing-library__jest-dom": "catalog:types",
    "@vue/test-utils": "catalog:dev",
    "vue": "catalog:prod"
  }
}
