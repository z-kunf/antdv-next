{
  "name": "antdv-next",
  "type": "module",
  "version": "1.0.0-rc.8",
  "description": "Ant Design Vue3 Component Library",
  "author": {
    "email": "aibayanyu@gamil.com",
    "name": "aibayanyu20"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antdv-next/antdv-next.git",
    "directory": "packages/antdv-next"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./dist/*": {
      "types": "./dist/*.d.mts",
      "import": "./dist/*.mjs",
      "default": "./dist/*.mjs"
    },
    "./dist/locale/*": {
      "types": "./dist/locale/*.d.mts",
      "import": "./dist/locale/*.mjs",
      "default": "./dist/locale/*.mjs"
    },
    "./locale/*": {
      "types": "./dist/locale/*.d.mts",
      "import": "./dist/locale/*.mjs",
      "default": "./dist/locale/*.mjs"
    },
    "./date-picker/*": {
      "types": "./dist/date-picker/*.d.mts",
      "import": "./dist/date-picker/*.mjs",
      "default": "./dist/date-picker/*.mjs"
    },
    "./date-picker/locale/*": {
      "types": "./dist/date-picker/locale/*.d.mts",
      "import": "./dist/date-picker/locale/*.mjs",
      "default": "./dist/date-picker/locale/*.mjs"
    },
    "./time-picker/*": {
      "types": "./dist/time-picker/*.d.mts",
      "import": "./dist/time-picker/*.mjs",
      "default": "./dist/time-picker/*.mjs"
    },
    "./time-picker/locale/*": {
      "types": "./dist/date-picker/locale/*.d.mts",
      "import": "./dist/date-picker/locale/*.mjs",
      "default": "./dist/date-picker/locale/*.mjs"
    },
    "./date-picker/generate": {
      "types": "./dist/date-picker/generate/index.d.mts",
      "import": "./dist/date-picker/generate/index.mjs",
      "default": "./dist/date-picker/generate/index.mjs"
    },
    "./date-picker/generate/*": {
      "types": "./dist/date-picker/generate/*.d.mts",
      "import": "./dist/date-picker/generate/*.mjs",
      "default": "./dist/date-picker/generate/*.mjs"
    },
    "./global.d.ts": "./global.d.ts",
    "./global": "./global.d.ts",
    "./dist/reset.css": "./dist/reset.css",
    "./dist/antd.css": "./dist/antd.css",
    "./web-types.json": "./web-types.json",
    "./web-tags.json": "./web-tags.json",
    "./package.json": "./package.json"
  },
  "unpkg": "dist/antd.js",
  "jsdelivr": "dist/antd.js",
  "files": [
    "dist",
    "global.d.ts",
    "package.json",
    "web-tags.json",
    "web-types.json"
  ],
  "contributes": {
    "html": {
      "customData": [
        "./web-tags.json"
      ]
    }
  },
  "scripts": {
    "test": "vitest run",
    "build": "run-s build:esm build:umd build:p",
    "build:p": "run-p build:web-types build:style",
    "build:esm": "tsdown",
    "build:umd": "vite build",
    "build:web-types": "esno ./scripts/web-types/index.ts",
    "build:llm": "esno ./scripts/llm/generate-llms.ts",
    "build:llm-semantic": "esno ./scripts/llm/generate-llms-semantic.ts",
    "build:token-meta": "esno ./scripts/token/generate-token-meta.ts",
    "build:token-statistic": "cross-env NODE_ENV=production CSSINJS_STATISTIC=1 esno ./scripts/token/collect-token-statistic.ts",
    "build:token": "run-p build:token-meta build:token-statistic",
    "build:style": "cross-env NODE_ENV=production  esno ./scripts/style/build-style.ts",
    "prepublish": "pnpm build",
    "bump": "bumpp --commit \"chore(release): antdv-next %s\" --push --tag \"antdv-next@%s\""
  },
  "dependencies": {
    "@ant-design/colors": "catalog:prod",
    "@ant-design/fast-color": "catalog:prod",
    "@antdv-next/cssinjs": "workspace:^",
    "@antdv-next/icons": "catalog:prod",
    "@v-c/async-validator": "catalog:vc",
    "@v-c/cascader": "catalog:vc",
    "@v-c/checkbox": "catalog:vc",
    "@v-c/collapse": "catalog:vc",
    "@v-c/color-picker": "catalog:vc",
    "@v-c/dialog": "catalog:vc",
    "@v-c/drawer": "catalog:vc",
    "@v-c/dropdown": "catalog:vc",
    "@v-c/image": "catalog:vc",
    "@v-c/input": "catalog:vc",
    "@v-c/input-number": "catalog:vc",
    "@v-c/mentions": "catalog:vc",
    "@v-c/menu": "catalog:vc",
    "@v-c/mutate-observer": "catalog:vc",
    "@v-c/notification": "catalog:vc",
    "@v-c/pagination": "catalog:vc",
    "@v-c/picker": "catalog:vc",
    "@v-c/progress": "catalog:vc",
    "@v-c/qrcode": "catalog:vc",
    "@v-c/rate": "catalog:vc",
    "@v-c/resize-observer": "catalog:vc",
    "@v-c/segmented": "catalog:vc",
    "@v-c/select": "catalog:vc",
    "@v-c/slick": "catalog:vc",
    "@v-c/slider": "catalog:vc",
    "@v-c/steps": "catalog:vc",
    "@v-c/switch": "catalog:vc",
    "@v-c/table": "catalog:vc",
    "@v-c/tabs": "catalog:vc",
    "@v-c/textarea": "catalog:vc",
    "@v-c/tooltip": "catalog:vc",
    "@v-c/tour": "catalog:vc",
    "@v-c/tree": "catalog:vc",
    "@v-c/tree-select": "catalog:vc",
    "@v-c/trigger": "catalog:vc",
    "@v-c/upload": "catalog:vc",
    "@v-c/util": "catalog:vc",
    "@v-c/virtual-list": "catalog:vc",
    "@vueuse/core": "catalog:prod",
    "dayjs": "catalog:prod",
    "defu": "catalog:prod",
    "es-toolkit": "catalog:prod",
    "scroll-into-view-if-needed": "catalog:prod",
    "throttle-debounce": "catalog:prod"
  },
  "web-types": "web-types.json",
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/throttle-debounce": "catalog:types",
    "csstype": "catalog:prod"
  }
}
